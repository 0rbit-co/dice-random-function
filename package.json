{
  "name": "./",
  "type": "module",
  "scripts": {
    "random-function:deploy": "cd ./ao/random-function && ./scripts/deploy.sh",
    "random-function:test": "cd ./ao/random-function && ./scripts/test.sh",
    "random-function:build": "cd ./ao/random-function && ./scripts/build.sh",
    "random-function:inject": "STATE_FILE=./ao/random-function/state-processes.yaml node ./utils/inject-process.js",
